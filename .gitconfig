[user]
	name = _ktakei
	email = lpc.www.com@gmail.com
[color]
	# colorの設定(以下のコマンドは自動で色つける)
	status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
[core]
	excludesfile = ~/.gitignore
	# autocrlf = input  # CRLFを自動変換
	editor = vim
	quotepath = false
[merge]
	tool = vimdiff
	ff = false
[alias]
	tree = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
